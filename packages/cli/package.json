{
	"name": "@package-pal/cli",
	"version": "0.0.40",
	"description": "CLI tool exposing core PackagePal functionality.",
	"keywords": [
		"package",
		"pal",
		"monorepo",
		"dependencies",
		"deps",
		"cli"
	],
	"license": "MIT",
	"type": "module",
	"bin": {
		"ppal": "bin/ppal",
		"dppal": "./src/index.ts"
	},
	"files": [
		"src/lib/install",
		"bin"
	],
	"dependencies": {
		"tar": "^7.5.1"
	},
	"devDependencies": {
		"@package-pal/core": "workspace:*",
		"@package-pal/util": "workspace:*",
		"@clack/prompts": "^0.11.0",
		"@stricli/core": "^1.2.1",
		"@types/bun": "^1.3.0",
		"typescript": "^5.9.3"
	},
	"optionalDependencies": {
		"@package-pal/cli-linux-x64": "0.0.12",
		"@package-pal/cli-linux-x64-musl": "0.0.12",
		"@package-pal/cli-linux-arm64": "0.0.12",
		"@package-pal/cli-linux-arm64-musl": "0.0.12",
		"@package-pal/cli-darwin-arm64": "0.0.12",
		"@package-pal/cli-darwin-x64": "0.0.12",
		"@package-pal/cli-windows-x64": "0.0.12"
	},
	"scripts": {
		"postinstall": "node ./src/lib/install/install-binary.js",
		"build": "cd ../ && bun run build cli"
	},
	"engines": {
		"node": ">=18"
	},
	"sideEffects": false
}
